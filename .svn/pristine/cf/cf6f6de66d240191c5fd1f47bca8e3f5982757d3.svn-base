							</imports>
							<dynevent>
								<![CDATA[
									String str = rrequest.getStringAttribute("suoshujigou","");
									System.out.println("***********************"+str);
									StringBuffer sbuffer=new StringBuffer();
									sbuffer.append("wx_winpage('upLoadImage.jsp?table=zbgl_zbwxjghz_pic&amp;tupianzhonglei=组织结构图&amp;tupiansuoshu=");
