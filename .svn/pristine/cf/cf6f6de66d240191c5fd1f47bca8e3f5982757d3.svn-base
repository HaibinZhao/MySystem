				<report id="zuzhijiegoutu_report" title="组织结构图" rowselect="single" depends="hangyeguanli_report" dependsparams="suoshujigou=@{jigoumingcheng}" type="editablelist2" pagesize="15" interceptor="com.wabacus.util.IdentifierGenerator_Interceptor" >
