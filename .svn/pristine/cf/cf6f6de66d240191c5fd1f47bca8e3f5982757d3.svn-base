				    <col column="neirongzhaiyao" label="摘要" datatype="clob" valuestyleproperty="colspan='3'">
				    	<inputbox type="textareabox" styleproperty="rows='4'"></inputbox>
				    </col>					    
				    <!-- <col column="{non-fromdb}" property="fujian" label="附件"></col> -->
				</display>
				<sql>
					<select>
				      <value>
					       <![CDATA[SELECT * FROM hyjs_hygli_guanlibanfa where  {#condition#} order by id]]>
				      </value>
				      <condition name="id" label="id" hidden="true">
					  		<value>
								<![CDATA[(id like  '%#data#%' )]]>
							</value>
					  	</condition>
			       </select>
			       <insert>
			            <params>
							<param name="suoshujigou" value="url{suoshujigou}"/>							
					    </params>
					    <value>
		  		             insert into hyjs_hygli_guanlibanfa(id=uuid{},@{mingcheng},suoshujigou=#{suoshujigou},@{pifushijian},@{neirongzhaiyao});
			           </value>
			       </insert>
			       <update>
			         update hyjs_hygli_guanlibanfa(@{mingcheng},@{pifushijian},@{neirongzhaiyao}) where id=@{id};
			  	   </update>
			       <delete>
			  	   		delete from hyjs_hygli_guanlibanfa where id=@{id__old};
			  	   </delete>
				</sql>
			</report>
		</page>
	</applications>