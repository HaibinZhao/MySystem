							</col>
							<col column="wenxing" datatype="clob" label="${wenxing}">
								<inputbox type="textareabox" styleproperty="rows='4' cols='140%' "></inputbox>
							</col>
							<col column="zongtidiancijianrong" datatype="clob" label="总体${diancijianrong}">
